<!--template swiper.wxml-->
<template name="swiperCon">
<swiper class="swiper-box"  indicator-dots="{{swiperCon.indicatorDots}}" 
        autoplay="true" interval="{{swiperCon.interval}}" duration="{{swiperCon.duration}}" >
    <block wx:for="{{swiperCon.imgUrls}}"wx:key='{{index}}'>
        <swiper-item>
            <image src="{{item}}"  class="slide-image" style='width:100%; overflow:show;'/>
        </swiper-item>
    </block>
</swiper>
</template>

